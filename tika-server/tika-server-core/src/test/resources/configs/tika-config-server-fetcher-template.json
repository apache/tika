{
  "fetchers": {
    "file-system-fetcher": {
      "file-system-fetcher": {
        "allowAbsolutePaths": true
      }
    }
  },
  "server": {
    "port": "PORT",
    "enableUnsecureFeatures": true,
    "endpoints": [
      "tika"
    ]
