{
  "fetchers": {
    "fsf": {
      "file-system-fetcher": {
        "basePath": "FETCHER_BASE_PATH",
        "extractFileSystemMetadata": false
      }
    }
  },
  "emitters": {
    "fse-json": {
      "file-system-emitter": {
        "basePath": "JSON_EMITTER_BASE_PATH",
        "fileExtension": "json",
        "onExists": "EXCEPTION"
      }
    },
    "fse-bytes": {
      "file-system-emitter": {
        "basePath": "BYTES_EMITTER_BASE_PATH",
        "fileExtension": "",
        "onExists": "EXCEPTION"
      }
    }
  },
  "server": {
    "port": 9999,
    "enableUnsecureFeatures": true,
    "endpoints": [
      "rmeta",
      "status",
      "async",
      "pipes"
    ]
  },
  "pipes": {
    "numClients": 2,
    "timeoutMillis": "TIMEOUT_MILLIS",
    "emitIntermediateResults": false,
    "forkedJvmArgs": [
      "-Xmx512m"
    ],
    "emitStrategy": {
      "type": "EMIT_ALL"
    }
  },
  "auto-detect-parser": {
    "outputThreshold": 1000000,
    "throwOnZeroBytes": false
  },
  "other-configs": {
    "digester-factory": {
      "commons-digester-factory": {
        "digests": [
          { "algorithm": "MD5" },
          { "algorithm": "SHA1", "encoding": "BASE32" }
        ]
      }
    }
  },
  "plugin-roots": "PLUGINS_PATHS"
}
