{
  "fetchers": {
    "file-system-fetcher": {
      "file-system-fetcher": {
        "allowAbsolutePaths": true
      }
    }
  },
  "server": {
    "port": 9999,
    "enableUnsecure": true,
    "endpoints": [
      "emit",
      "async",
      "status"
    ]
  },
  "pipes": {
    "numClients": 2,
    "timeoutMillis": 120000,
    "forkedJvmArgs": [
      "-Xmx256m"
    ]
  },
  "plugin-roots": "target/plugins"
}
