{
  "parsers": [
    {
      "default-parser": {}
    },
    {
      "pdf-parser": {
        "sortByPosition": true
      }
    }
  ],
  "metadata-filters": [
    {
      "open-nlp-metadata-filter": {}
    }
  ],
  "auto-detect-parser": {
    "outputThreshold": 1000000,
    "throwOnZeroBytes": false
  },
  "other-configs": {
    "digester-factory": {
      "commons-digester-factory": {
        "digests": [
          { "algorithm": "MD5" },
          { "algorithm": "SHA1", "encoding": "BASE32" }
        ]
      }
    }
  },
  "fetchers": {
    "file-system-fetcher": {
      "file-system-fetcher": {
        "allowAbsolutePaths": true
      }
    }
  },
  "pipes": {
    "numClients": 2,
    "timeoutMillis": 30000
  },
  "plugin-roots": "target/plugins"
}
