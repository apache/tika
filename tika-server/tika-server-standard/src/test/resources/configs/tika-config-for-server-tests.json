{
  "parsers": [
    {
      "default-parser": {}
    },
    {
      "pdf-parser": {
        "sortByPosition": true
      }
    }
  ],
  "auto-detect-parser": {
    "spoolToDisk": 1000000,
    "outputThreshold": 1000000,
    "digesterFactory": {
      "commons-digester-factory": {
        "markLimit": 100000,
        "digests": [
          { "algorithm": "MD5" },
          { "algorithm": "SHA1", "encoding": "BASE32" }
        ]
      }
    },
    "throwOnZeroBytes": false
  }
}
