{
  "fetchers": {
    "fsf": {
      "file-system-fetcher": {
        "basePath": "FETCHER_BASE_PATH",
        "extractFileSystemMetadata": false
      }
    }
  },
  "emitters": {
    "fse": {
      "file-system-emitter": {
        "basePath": "EMITTER_BASE_PATH",
        "fileExtension": "json",
        "onExists": "EXCEPTION"
      }
    }
  },
  "pipes": {
    "numClients": 1,
    "forkedJvmArgs": [
      "-Xmx256m"
    ],
    "emitStrategy": {
      "type": "DYNAMIC",
      "thresholdBytes": 1000000
    }
  },
  "parse-context": {
    "timeout-limits": {
      "progressTimeoutMillis": 5000
    }
  },
  "detectors": [
    {
      "org.apache.tika.pipes.core.CrashingDetector": {}
    }
  ],
  "plugin-roots": "PLUGINS_PATHS"
}
