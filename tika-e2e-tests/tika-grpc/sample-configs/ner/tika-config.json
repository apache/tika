{
  "async": {
    "staleFetcherTimeoutSeconds": 600,
    "staleFetcherDelaySeconds": 60
  },
  "pipes": {
    "numClients": 2,
    "forkedJvmArgs": [
      "-Xmx1g",
      "-XX:ParallelGCThreads=2"
    ],
    "maxForEmitBatchBytes": -1
  },
  "parsers": [
    {
      "class": "org.apache.tika.parser.ner.NamedEntityParser",
      "supportedMimeTypes": [
        "application/pdf",
        "text/plain",
        "text/html",
        "application/xhtml+xml"
      ]
    }
  ]
}
