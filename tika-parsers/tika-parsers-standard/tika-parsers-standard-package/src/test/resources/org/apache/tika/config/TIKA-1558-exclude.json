{
  "parsers": [
    {
      "default-parser": {
        "exclude": ["executable-parser"],
        "_decorate": {
          "mimeExclude": ["image/jpeg", "application/pdf"]
        }
      }
    },
    {
      "empty-parser": {
        "_decorate": {
          "mimeInclude": ["application/pdf"]
        }
      }
    }
  ]
}
