{
  "metadata-filters": [
    {
      "include-field-metadata-filter" : {
        "include": [
          "X-TIKA:content",
          "extended-properties:Application",
          "Content-Type"
        ]
      }
    },
    {
      "remove-by-mime-metadata-filter": {
        "mimes": [
          "image/emf",
          "text/plain"
        ]
      }
    }
  ]
}
