{
  "auto-detect-parser": {
    "outputThreshold": 1000000,
    "throwOnZeroBytes": false
  },
  "other-configs": {
    "digester-factory": {
      "bouncy-castle-digester-factory": {
        "digests": [
          { "algorithm": "MD2" },
          { "algorithm": "MD5" },
          { "algorithm": "SHA1" },
          { "algorithm": "SHA256" },
          { "algorithm": "SHA384" },
          { "algorithm": "SHA512" }
        ]
      }
    }
  }
}
