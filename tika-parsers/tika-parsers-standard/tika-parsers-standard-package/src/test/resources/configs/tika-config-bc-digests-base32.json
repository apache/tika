{
  "auto-detect-parser": {
    "spoolToDisk": 1000000,
    "outputThreshold": 1000000,
    "digesterFactory": {
      "bouncy-castle-digester-factory": {
        "markLimit": 1000000,
        "digests": [
          { "algorithm": "SHA1", "encoding": "BASE32" }
        ]
      }
    },
    "throwOnZeroBytes": false
  }
}
