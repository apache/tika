{
  "parsers": [
    {
      "default-parser": {
        "exclude": [
          "pdf-parser"
        ]
      }
    }
  ],
  "auto-detect-parser": {
    "outputThreshold": 1000000,
    "digesterFactory": {
      "commons-digester-factory": {
        "markLimit": 100000,
        "digests": [
          { "algorithm": "SHA256", "encoding": "BASE32" },
          { "algorithm": "MD5" }
        ]
      }
    },
    "throwOnZeroBytes": false
  }
}
