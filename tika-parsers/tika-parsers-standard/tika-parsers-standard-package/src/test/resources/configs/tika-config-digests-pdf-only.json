{
  "parsers": [
    {
      "default-parser": {
        "exclude": [
          "pdf-parser"
        ]
      }
    }
  ],
  "auto-detect-parser": {
    "throwOnZeroBytes": false
  },
  "parse-context": {
    "commons-digester-factory": {
        "digests": [
          { "algorithm": "SHA256", "encoding": "BASE32" },
          { "algorithm": "MD5" }
        ]
      }
    }
}
