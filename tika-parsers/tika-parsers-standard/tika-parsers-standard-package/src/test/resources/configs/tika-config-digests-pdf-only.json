{
  "parsers": [
    {
      "default-parser": {
        "_exclude": [
          "pdf-parser"
        ]
      }
    }
  ],
  "auto-detect-parser": {
    "spoolToDisk": 1000000,
    "outputThreshold": 1000000,
    "digesterFactory": {
      "commons-digester-factory": {
        "markLimit": 100000,
        "algorithmString": "sha256:32,md5"
      }
    },
    "throwOnZeroBytes": false
  }
}
